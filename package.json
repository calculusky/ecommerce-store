        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.0",
        "moment-logger": "1.0.5-12",
        "morgan": "^1.10.0",
        "nanoid": "3.3.4",
        "yup": "^1.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.3",
        "@commitlint/config-conventional": "^17.6.3",
